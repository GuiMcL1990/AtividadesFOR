public class Ativ11 {
    public static void main(String[] args) {
        int count = 0;
        int num = 2;
        
        do {
            int soma = 0;
            for (int i = 1; i <= num / 2; i++) {
                if (num % i == 0) {
                    soma += i;
                }
            }
            if (soma == num) {
                System.out.println(num);
                count++;
            }
            num++;
        } while (count < 10);
    }
}

